{
  "name": "vscode-angular-snippets",
  "displayName": "vscode-angular-snippets",
  "description": "Visual Studio Code Snippets for Angular Development",
  "version": "0.0.0",
  "publisher": "markdegroot",
  "engines": {
    "vscode": "^1.72.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/markdegroot/vscode-angular-snippets.git"
  },
  "homepage": "https://markdegroot.info",
  "scripts": {
    "package": "npx vsce package",
    "publish": "npx vsce publish",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "keywords": [
    "html",
    "javascript",
    "typescript",
    "angular"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      }
    ]
  }
}
