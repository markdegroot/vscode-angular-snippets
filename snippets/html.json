{
  "ngFor": {
    "prefix": "x-ngFor",
    "body": ["*ngFor=\"let ${1:item} of ${2:list}\"${0}"],
    "description": "Angular *ngFor"
  },
  "ngFor with trackBy": {
    "prefix": "x-ngFor-trackBy",
    "body": ["*ngFor=\"let ${1:item} of ${2:list}; trackBy:${1:item}.id\"${0}"],
    "description": "Angular *ngFor with trackBy"
  }
}
